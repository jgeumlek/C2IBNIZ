unsigned int DATA[] = {
  0b00000000000000000000000000000000,
  0b00000011100111110011111001000100,
  0b00000100100000010000001001000100,
  0b00001000100111110000001001000100,
  0b00000100100100000000001001000100,
  0b00000011100111110011111001111100,
  0b00000000000000000000000000000000,
  0b00000000000000000000000000000000,
};


int ibniz_run(int time, int x, int y) {
  int wave = sin(time >> 6);
  x *= wave;
  y *= wave;
  return fixed16(0,0x8000) & (DATA[y & 7] >> x);
}

